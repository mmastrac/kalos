handle init {
    var foo = 0;
    loop {
        print("start loop!\n");
        foo = foo + 1;
        print(foo);
        print("\n");
        if (foo == 3) {
            break;
        } else if (foo == 2) {
            continue;
        }
        print("end loop!\n");
    }
    print("done!\n");
}
